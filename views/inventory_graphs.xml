<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record model="ir.ui.view" id="inventory_data_makanan_graph">
        <field name="name">inventory.data.makanan.graph</field>
        <field name="model">inventory.data.makanan</field>
        <field name="arch" type="xml">
            <graph string="Makanan Graph">
                <field name="type" type="row"/>
                <field name="id" type="measure"/>
            </graph>
        </field>
    </record>

    <record model="ir.ui.view" id="inventory_data_pembelian_bahan_mentah_graph">
        <field name="name">inventory.data.pembelian_bahan_mentah.graph</field>
        <field name="model">inventory.data.pembelian_bahan_mentah</field>
        <field name="arch" type="xml">
            <graph string="Pembelian Bahan Mentah Graph">
                <group expand="0" string="Satuan">
                    <field name="unit"/>
                    <field name="name" type="row"/>
                    <field name="quantity" type="measure"/>
                </group>
            </graph>
        </field>
    </record>


</odoo>